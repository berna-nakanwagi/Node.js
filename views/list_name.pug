doctype html
html
  head
    title Our Users
    link(rel="stylesheet" href="/css/table.css")
  body
    h2 Our users
    table
      tr
        th Username
        th Email
        th Password
        th Role
      if users.length > 0
        each user in users
          tr
            td #{user.username}
            td #{user.email}
            td #{user.password}
            td #{user.role}

  